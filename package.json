{
  "name": "okto-miro",
  "version": "1.0.1",
  "license": "Attribution-NonCommercial-ShareAlike 4.0 International",
  "scripts": {
    "build": "./scripts/extract-version.sh dotenv -e .env.local -- preact build --no-prerender --no-sw --template src/index.html --dest ./dist",
    "serve:prod": "sirv dist -p 3000 --cors --single --host 0.0.0.0",
    "serve:dev": "preact watch -p 3000 --template src/index.html",
    "dev": "./scripts/extract-version.sh dotenv -e .env.local -- preact watch -p 3000 --template src/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update-token-exports": "scripts/update-token-exports.sh",
    "format:fix": "yarn lint --fix; npx prettier --write src",
    "lint": "eslint",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "mirotone": "5",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "react-tabs": "^6.0.2",
    "throttle-debounce": "^5.0.2",
    "ttrpg-lib-dice": "^2.0.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@mirohq/websdk-types": "latest",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/preact": "^3.2.4",
    "@types/jest": "^30.0.0",
    "@types/throttle-debounce": "^5.0.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3",
    "ts-jest": "^29.4.1",
    "typescript-eslint": "^8.9.0"
  }
}
