  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@mirohq/websdk-types": "latest",
    "@types/throttle-debounce": "^5.0.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "3.0.1",
    "typescript-eslint": "^8.39.0"
